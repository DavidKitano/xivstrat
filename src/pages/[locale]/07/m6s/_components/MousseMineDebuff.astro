---
import Buff from '@/components/Buff.astro'

import mousse_mine from '@/assets/buff/07/m6s/214277.png'

interface Props {
  tag?: string
  hover?: boolean
  class?: string
}

const { tag = '', hover = false, class: className = '' }: Props = Astro.props
---

<Buff
  icon={mousse_mine}
  name="Mousse Mine"
  nameVariant="yellow"
  description="Covered in mousse and unable to move. Will detonate when this effect expires."
  tag={tag}
  hover={hover}
  class={className}
/>
